        <w lemma="old" pos="j" xml:id="A19883-003-a-0150">old</w>
        <pc xml:id="A19883-003-a-0160">,</pc>
        <w lemma="that" pos="cs" xml:id="A19883-003-a-0170">that</w>
        <w lemma="play" pos="vvz" xml:id="A19883-003-a-0180">
