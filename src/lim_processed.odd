      <choice>
        <abbr>SPQR</abbr>
        <expan xml:lang="la">senatus populusque romanorum</expan>
      </choice>
    </egXML>
  </exemplum><exemplum xml:lang="zh-TW">
    <egXML xmlns="http://www.tei-c.org/ns/Examples" source="#NONE">
      <abbr>北大</abbr>
    </egXML>
  </exemplum><exemplum xml:lang="zh-TW">
    <egXML xmlns="http://www.tei-c.org/ns/Examples" source="#NONE">
      <choice>
        <abbr>北大</abbr>
        <expan>北京大學</expan>
