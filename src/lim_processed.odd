                    <sequence minOccurs="1" maxOccurs="1"><elementRef key="persName" minOccurs="1" maxOccurs="unbounded"/><sequence minOccurs="1" maxOccurs="1" preserveOrder="false"><elementRef key="sex" minOccurs="0" maxOccurs="1"/><elementRef key="birth" minOccurs="0" maxOccurs="1"/><elementRef key="death" minOccurs="0" maxOccurs="1"/><elementRef key="faith" minOccurs="0" maxOccurs="unbounded"/><elementRef key="affiliation" minOccurs="0" maxOccurs="unbounded"/><elementRef key="occupation" minOccurs="0" maxOccurs="unbounded"/><elementRef key="nationality" minOccurs="0" maxOccurs="unbounded"/><elementRef key="residence" minOccurs="0" maxOccurs="unbounded"/><elementRef key="figure" minOccurs="0" maxOccurs="unbounded"/></sequence><elementRef key="langKnowledge" minOccurs="0" maxOccurs="1"/><elementRef key="note" minOccurs="0" maxOccurs="1"/></sequence> 
                  </content>
               </macroSpec><dataSpec rend="add" module="lim" ident="limdata.ana" mode="add">
                  <desc xmlns:xsl="http://www.w3.org/1999/XSL/Transform">Datatype for all analytic pointers</desc>
                  <content xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
